    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rsg-components": "^3.0.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-device-detect": "^1.13.1",
    "rollup-plugin-browsersync": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=true npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint",
      "git add"
    ],
    "scripts/**/*.{js,jsx}": [
      "eslint",
      "git add"
    ],
    "*.css": [
      "stylelint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AwesomeWebProjects/react-player.git"
  },
  "keywords": [
    "react",
    "js",
    "canvas",
    "audio",
    "web",
    "audio",
    "api",
    "component"
  ]
}
