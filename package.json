{
  "name": "@AwesomeWebProjects/react-player",
  "description": "Web Audio Player as React Component",
  "version": "1.0.0",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "browser": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "(rm -rf build || rd /s /q build) && mkdir build && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.dev.js --watch",
    "eslint": "eslint --ext=js --ext=jsx --fix ./src ./scripts",
    "stylelint": "stylelint \"src/**/*.css\"",
    "prettier": "prettier --config ./.prettierrc.json --write \"**/*{.js,.jsx,.css}\"",
    "build": "(rm -rf build || rd /s /q build) && mkdir build && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.prod.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.7.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.0.0",
    "acorn": "^7.3.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-root-import": "^6.5.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
